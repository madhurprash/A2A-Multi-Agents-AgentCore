# OpenAI API Key - Required for agent operations
OPENAI_API_KEY=sk-your-openai-api-key-here

# Tavily API Key - Required for web search functionality  
TAVILY_API_KEY=tvly-your-tavily-api-key-here

# JIRA API Key - Optional, for JIRA integration
JIRA_API_KEY=your-jira-api-token-here

# AWS Configuration
AWS_REGION=us-west-2
AWS_PROFILE=default

# AgentCore Configuration
AGENT_OBSERVABILITY_ENABLED=true
OTEL_PYTHON_DISTRO=aws-opentelemetry
OTEL_PYTHON_CONFIGURATOR=aws-opentelemetry-configurator
OTEL_EXPORTER_OTLP_PROTOCOL=grpc
OTEL_TRACES_EXPORTER=otlp